@model HomeVM
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<!--====== Slider Part Start ======-->
<div class="position-relative olo">
    <div class="slider">
        @foreach (var slider in Model.Sliders)
        {

            <div class="py-5 py-sm-100" style="background-image: url(assets/img/slider/slider-bg.png); ">
                <div class="container">
                    <div class="row align-items-center justify-content-center">
                        <div class="col-lg-7 ">
                            <h1 class=" text-white fs-70 fs-sm-32 animate__animated animate__fadeInLeft animate__slow">
                                @Html.Raw(slider.Header)
                            </h1>
                            <div class="slider-border"></div>
                            <p class="animate__animated animate__fadeInLeft  animate__slow work-sans pb-4 fs-5 fs-sm-16 pt-3 text-white">
                                @Html.Raw(slider.Title)
                            </p>
                            <ul class="list-unstyled">
                                <li class="d-inline-block animate__animated animate__fadeInLeft  animate__slower">
                                    <a href="#"
                                   class="bg-transparent animate__animated animate__fadeInLeft  animate__slow border br-50 px-5 py-3  text-decoration-none text-white hover3 t-5">
                                        Explore
                                        More <i class="fa-solid fa-cart-shopping ps-3"></i>
                                    </a>
                                </li>
                                <li class="d-inline-block ps-4  animate__animated animate__fadeInLeft  animate__slower">
                                    <a class=" hover2 ripple  text-decoration-none popup" data-toggle="modal"
                                   style="animation-delay: 2s" href="#myModal">
                                        <i class="fas fa-play"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="col-lg-5 col-7 col-sm-7 ">
                            <img src="~/assets/img/slider/@slider.ImageUrl"
                             class="animate__animated animate__fadeInLeft  animate__slow w-100" alt="">
                        </div>
                    </div>
                </div>
            </div>
        }

    </div>
    <div class="slick-slider-dots pas pas-sm"></div>
    <div class="prev prev-sm pa pa-sm t-5">
        <i class="fa-solid fa-chevron-left work-sans" style="cursor: pointer;"></i>
    </div>
    <div class="next next-sm  pa1 pa1-sm">
        <i class="fa-solid fa-chevron-right work-sans" style="cursor: pointer;"></i>
    </div>
</div>
<!--====== Slider Part End ======-->
