<div class="blog-area py-5">
    <div class="container">
        <div class="section-title text-center">
            <h2 class="work-sans text-white ">Latest Blog</h2>
            <p class="work-sans text-white ">Contrary to popular belief, Lorem Ipsum is not simply random.</p>
        </div>
        <div class="">
            <div class="latest-blog-slider row">
                @foreach (var blog in Model.Blogs)
               {
                   
                <div class="">
                    <div class="blog-wrap mx-3 ms-sm-4 border py-2 px-2 text-center">
                        <div class="blog-img ">
                            <a href="/blog/detail/@blog.Id">
                                <img src="~/assets/img/blog/@blog.ImageUrl" alt="Significant reading has info" class="">
                            </a>
                        </div>
                        <div class="blog-content px-2 text-center">
                            <h5 class="py-3 border-bottom">
                                <a href="/blog/detail/@blog.Id"
                                   class="text-decoration-none text-white hover2 t-5 geometric ls-1 ">
                                   @blog.Title
                                </a>
                            </h5>
                            <div class="blog-meta pt-2">
                                <ul class="list-unstyled">
                                    <li class="d-inline-block">
                                        <a class="text-decoration-none text-grey hover2 t-5">@blog.CreatedTime.ToString("dd MMMM, yyyy")</a>
                                    </li>
                                    <li class="d-inline-block">
                                        <span class="text-grey ">\</span>
                                    </li>
                                    <li class="d-inline-block">
                                        <a href="" class="text-decoration-none hover2 t-5 text-grey"> @blog.Comments.Count() comment</a>
                                    </li>
                                </ul>
                            </div>
                            <p class="text-white work-sans pb-3 ">
                                @blog.Description.Substring(0,96)...
                            </p>
                            <div class="read-more-btn pb-4">
                                <a href="/blog/detail/@blog.Id" href="/blog/detail/@blog.Id"
                                   class="text-decoration-none text-white px-4 py-3 border work-sans hover3 t-5 br-50">
                                    Read
                                    More
                                </a>
                            </div>
                        </div>
                    </div>
                </div> 
               }
            </div>
        </div>
    </div>
</div>
<!--====== Latest Blog Area End ======-->